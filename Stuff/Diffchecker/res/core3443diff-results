-        if (fsort.invalidKey()) {
-            fsortEof.set("J");
-        }
-    }
-
-    private void fsortReadnext() {
-        fsortEof.set("N");
-        fsort.readNext();
-        if (fsort.atEnd()) {
-            fsortEof.set("J");
-        }
-        if (fsortFs.equals("9D")) {
-            fsort.start(">", "fsort_key");
-            c_sleep("1");
-            fsortReadnext();
-            return;
-        }
-    }
