-                proDefkFilnamn.set(SPACE);
-                proDefkFilnamn.set(concat("DTA/",limit(pa.bkstvftg,SPACE),"/PRO/FIL-5"));
-                proDefk.open("I-O");
-                proCheck00();
-                proDefkOpen.set("J");
-                proDefkOpenFtg.set(pa.bkstvftg);
